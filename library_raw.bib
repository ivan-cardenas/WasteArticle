@article{Fritze2008,
  title = {Hope, Despair and Transformation: {{Climate}} Change and the Promotion of Mental Health and Wellbeing},
  author = {Fritze, Jessica C. and Blashki, Grant A. and Burke, Susie and Wiseman, John},
  year = {2008},
