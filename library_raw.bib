@article{Moral2022,
  title = {Towards Automatic Waste Containers Management in Cities via Computer Vision: Containers Localization and Geo-Positioning in City Maps},
  author = {Moral, Paula and {Garc{\'i}a-Mart{\'i}n}, {\'A}lvaro and {Escudero-Vi{\~n}olo}, Marcos and Mart{\'i}nez, Jos{\'e} M. and Besc{\'o}s, Jes{\'u}s and Pe{\~n}uela, Jes{\'u}s and Mart{\'i}nez, Juan Carlos and Alvis, Gonzalo},
