@article{Sloan2015,
  title = {Who Tweets with Their Location? {{Understanding}} the Relationship between Demographic Characteristics and the Use of Geoservices and Geotagging on Twitter},
  author = {Sloan, Luke and Morgan, Jeffrey},
  year = {2015},
  month = nov,
  journal = {PLOS ONE},
  volume = {10},
  number = {11},
  pages = {e0142209},
  publisher = {{Public Library of Science}},
  issn = {1932-6203},
  doi = {10.1371/JOURNAL.PONE.0142209},
  abstract = {In this paper we take advantage of recent developments in identifying the demographic characteristics of Twitter users to explore the demographic differences between those who do and do not enable location services and those who do and do not geotag their tweets. We discuss the collation and processing of two datasets{\textemdash}one focusing on enabling geoservices and the other on tweet geotagging. We then investigate how opting in to either of these behaviours is associated with gender, age, class, the language in which tweets are written and the language in which users interact with the Twitter user interface. We find statistically significant differences for both behaviours for all demographic characteristics, although the magnitude of association differs substantially by factor. We conclude that there are significant demographic variations between those who opt in to geoservices and those who geotag their tweets. Not withstanding the limitations of the data, we suggest that Twitter users who publish geographical information are not representative of the wider Twitter population.},
  pmid = {26544601},
  keywords = {Culture,Korean people,Language,Professions,Routes of administration,Social stratification,Twitter,User interfaces}
}

@article{Smith2014,
  title = {The Role of Emotion in Global Warming Policy Support and Opposition},
  author = {Smith, Nicholas and Leiserowitz, Anthony},
  year = {2014},
  month = may,
  journal = {Risk Analysis},
  volume = {34},
  number = {5},
  pages = {937--948},
  publisher = {{John Wiley \& Sons, Ltd}},
  issn = {1539-6924},
  doi = {10.1111/RISA.12140},
  abstract = {Prior research has found that affect and affective imagery strongly influence public support for global warming. This article extends this literature by exploring the separate influence of discrete emotions. Utilizing a nationally representative survey in the United States, this study found that discrete emotions were stronger predictors of global warming policy support than cultural worldviews, negative affect, image associations, or sociodemographic variables. In particular, worry, interest, and hope were strongly associated with increased policy support. The results contribute to experiential theories of risk information processing and suggest that discrete emotions play a significant role in public support for climate change policy. Implications for climate change communication are also discussed. {\textcopyright} 2013 Society for Risk Analysis.},
  pmid = {24219420},
  keywords = {Emotion,global warming,policy preferences}
}

@article{Smith2018,
  title = {Social Media Dialogues in a Crisis: {{A}} Mixed-Methods Approach to Identifying Publics on Social Media},
  author = {Smith, Brian G. and Smith, Staci B. and Knighton, Devin},
  year = {2018},
  month = nov,
  journal = {Public Relations Review},
  volume = {44},
  number = {4},
  pages = {562--573},
  publisher = {{JAI}},
  issn = {03638111},
  doi = {10.1016/j.pubrev.2018.07.005},
  abstract = {Despite the promise of social media to engender dialogue, the common approach to studying social media may prioritize monologue, whereby research considers the strategies organizations use in targeting publics, particularly in a crisis. This study uses a mixed-method approach to analyze dialogue in a crisis{\textemdash}semantic network analysis and content analysis. Specifically, this study examines the emotional expression and crisis coping behaviors on social media during two separate terror attacks: the Paris terror attacks in 2015 and the Barcelona terror attacks in 2017. Results demonstrate how publics may be identified and understood through semantic network analysis and content analysis. This study also shows the connection between emotions and coping, expanding the crisis communication literature in public relations, and suggests the need to consider agenda-setting and resilience in crisis communication research. Finally, we discuss this study's implications for assuming a dialogic orientation in public relations.},
  keywords = {Crisis communication,Dialogue,Mixed methods,Publics,Semantic network analysis,Social media}
}

@article{Spence2012,
  title = {The Psychological Distance of Climate Change},
  author = {Spence, Alexa and Poortinga, Wouter and Pidgeon, Nick},
  year = {2012},
  month = jun,
  journal = {Risk Analysis},
  volume = {32},
  number = {6},
  pages = {957--972},
  publisher = {{John Wiley \& Sons, Ltd}},
  issn = {1539-6924},
  doi = {10.1111/J.1539-6924.2011.01695.X},
  abstract = {Avoiding dangerous climate change is one of the most urgent social risk issues we face today and understanding related public perceptions is critical to engaging the public with the major societal transformations required to combat climate change. Analyses of public perceptions have indicated that climate change is perceived as distant on a number of different dimensions. However, to date there has been no in-depth exploration of the psychological distance of climate change. This study uses a nationally representative British sample in order to systematically explore and characterize each of the four theorized dimensions of psychological distance-temporal, social, and geographical distance, and uncertainty-in relation to climate change. We examine how each of these different aspects of psychological distance relate to each other as well as to concerns about climate change and sustainable behavior intentions. Results indicate that climate change is both psychologically distant and proximal in relation to different dimensions. Lower psychological distance was generally associated with higher levels of concern, although perceived impacts on developing countries, as an indicator of social distance, was also significantly related to preparedness to act on climate change. Our findings clearly point to the utility of risk communication techniques designed to reduce psychological distance. However, highlighting the potentially very serious distant impacts of climate change may also be useful in promoting sustainable behavior, even among those already concerned. {\textcopyright} 2011 Society for Risk Analysis.},
  pmid = {21992607},
  keywords = {climate change,global warming,psychological distance,risk perceptions,sustainable behavior}
}

