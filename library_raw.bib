@article{Converse2022,
  title = {People-Watching and the Environment: {{Looking}} for Signs of Hope While Concern Outpaces Action.},
  author = {Converse, Benjamin A and Austin, Maura M K},
  year = {2022},
  month = feb,
  journal = {Current opinion in psychology},
  volume = {43},
  eprint = {34481331},
  eprinttype = {pubmed},
  pages = {249--253},
  publisher = {{Elsevier}},
  issn = {2352-2518},
  doi = {10.1016/j.copsyc.2021.08.001},
  abstract = {The social landscape of climate change is shifting. As more people acknowledge the urgency of the problem and society's underwhelming response to it, climate despair threatens to become a major contributor to personal inaction. At this moment, people need a reason for hope. Recent research shows that climate hope, where it exists, is largely social: People feel hopeful when they see others taking action. 'People-watching' is therefore critical. But capitalizing on social proof is tricky as long as concern outpaces action. Other recent research elegantly responds to this conundrum by advertising improvements in social norms, setting people up to find cause for hope even when action is low. We close by nominating other possible social paths to increased hope.},
  pmid = {34481331},
  keywords = {Climate,Decision making,Motivation,Proenvironmental behavior,Social norms}
}

